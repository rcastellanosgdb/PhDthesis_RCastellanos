% COMMON FUNCTIONS 
\newcommand\shcommandassign[1]{
\ifthenelse{\equal{#1}{t}}{\let\mysh\myemptytriangle}{% 
\ifthenelse{\equal{#1}{t*}}{\let\mysh\mytriangle}{%
\ifthenelse{\equal{#1}{dt}}{\let\mysh\myemptydowntriangle}{% 
\ifthenelse{\equal{#1}{dt*}}{\let\mysh\mydowntriangle}{% 
\ifthenelse{\equal{#1}{rt}}{\let\mysh\myemptyrighttriangle}{% 
\ifthenelse{\equal{#1}{rt*}}{\let\mysh\myrighttriangle}{%
\ifthenelse{\equal{#1}{lt}}{\let\mysh\myemptylefttriangle}{% 
\ifthenelse{\equal{#1}{lt*}}{\let\mysh\mylefttriangle}{% 
\ifthenelse{\equal{#1}{o*}}{\let\mysh\mycircle}{%
\ifthenelse{\equal{#1}{o}}{\let\mysh\myemptycircle}{%
\ifthenelse{\equal{#1}{s*}}{\let\mysh\mysquare}{%  
\ifthenelse{\equal{#1}{s}}{\let\mysh\myemptysquare}{%  
\ifthenelse{\equal{#1}{p}}{\let\mysh\myemptypentagram}{%  
\ifthenelse{\equal{#1}{*}}{\let\mysh\myasterisk}{%
\ifthenelse{\equal{#1}{d}}{\let\mysh\myemptydiamond}{%
\ifthenelse{\equal{#1}{d*}}{\let\mysh\mydiamond}{%
\ifthenelse{\equal{#1}{+}}{\let\mysh\myplus}{%
\ifthenelse{\equal{#1}{x}}{\let\mysh\mycross}{% 
\ifthenelse{\equal{#1}{rec}}{\let\mysh\myrectangle}{% 
\ifthenelse{\equal{#1}{none}}{\let\mysh\myemptysymbol}{
}}}}}}}}}}}}}}}}}}}}
}


